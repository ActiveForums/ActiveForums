/* Added this lines to delete Artifacts that still in database - added by Matthias Schlomann */
IF EXISTS (SELECT * FROM {databaseOwner}{objectQualifier}CoreMessaging_NotificationTypeActions where APICall = 'DesktopModules/ActiveForums/API/ModerationService.ashx/ApprovePost')
DELETE FROM {databaseOwner}[{objectQualifier}CoreMessaging_NotificationTypeActions]
      WHERE 
	  APICall = 'DesktopModules/ActiveForums/API/ModerationService.ashx/ApprovePost' 
GO
IF EXISTS (SELECT * FROM {databaseOwner}{objectQualifier}CoreMessaging_NotificationTypeActions where APICall = 'DesktopModules/ActiveForums/API/ModerationService.ashx/RejectPost')
DELETE FROM {databaseOwner}[{objectQualifier}CoreMessaging_NotificationTypeActions]
      WHERE 
	  APICall = 'DesktopModules/ActiveForums/API/ModerationService.ashx/RejectPost' 
GO
IF EXISTS (SELECT * FROM {databaseOwner}{objectQualifier}CoreMessaging_NotificationTypeActions where APICall = 'DesktopModules/ActiveForums/API/ModerationService.ashx/DeletePost')
DELETE FROM {databaseOwner}[{objectQualifier}CoreMessaging_NotificationTypeActions]
      WHERE 
	 
	  APICall = 'DesktopModules/ActiveForums/API/ModerationService.ashx/DeletePost'
GO
IF EXISTS (SELECT * FROM {databaseOwner}{objectQualifier}CoreMessaging_NotificationTypeActions where APICall = 'DesktopModules/ActiveForums/API/ModerationService.ashx/IgnorePost')
DELETE FROM {databaseOwner}[{objectQualifier}CoreMessaging_NotificationTypeActions]
      WHERE 
	  APICall = 'DesktopModules/ActiveForums/API/ModerationService.ashx/IgnorePost' 
GO
IF EXISTS (select * FROM {databaseOwner}{objectQualifier}CoreMessaging_NotificationTypes where Name = 'AF-ForumModeration' )

DELETE FROM {databaseOwner}[{objectQualifier}CoreMessaging_NotificationTypes]
      WHERE 
	  Name = 'AF-ForumModeration' 
GO

IF EXISTS (select * FROM {databaseOwner}[{objectQualifier}CoreMessaging_NotificationTypes] where Name = 'AF-ContentAlert' )

DELETE FROM {databaseOwner}[{objectQualifier}CoreMessaging_NotificationTypes]
      WHERE 
	  Name ='AF-ContentAlert'
